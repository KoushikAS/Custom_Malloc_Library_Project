#include <unistd.h>

struct memory_block_list {
  void *start_address;
  void *end_address;
  int len;
  struct memory_block_list *next;
  struct memory_block_list *previous;
};

typedef struct memory_block_list memory_block_list; 

extern memory_block_list *head = NULL;

void *ff_malloc(size_t size);
void ff_free(void * ptr);

